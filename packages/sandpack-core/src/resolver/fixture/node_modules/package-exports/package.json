{
  "name": "package-exports",
  "main": "dist.js",
  "exports": {
    ".": "./module.js",
    "./something": {
      "browser": "./foo.js",
      "node": "./node.js"
    },
    "./internal": null,
    "./components/*": "./src/components/*.js",
    "./utils/*": {
      "development": "./src/utils/*.js",
      "production": "./src/utils/*.min.js"
    }
  }
}
