    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "meta.selector",
      "settings": {
        "foreground": "#d7ba7d"
      }
    },
    {
      "name": "brackets of XML/HTML tags",
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": {
        "foreground": "#ce9178"
      }
