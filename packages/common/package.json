    "ot": "^0.0.15",
    "react-icons": "^2.2.7",
    "react-input-autosize": "^2.2.1",
    "react-textarea-autosize": "^6.1.0",
    "react-tippy": "^0.14.0",
    "styled-components": "^4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@types/color": "0.12.1",
    "@types/jest": "^24.0.9",
    "@types/react": "^16.8.6",
    "@types/react-icons": "^2.2.7",
    "@types/styled-components": "^4.1.11",
    "babel-jest": "^23.6.0",
