            "skipFiles": {
              "type": "array",
              "description": "%node.skipFiles.description%",
              "items": {
                "type": "string"
              },
              "default": []
            },
            "trace": {
              "type": ["boolean", "string"],
              "description": "%node.trace.description%",
              "default": true
            },
            "enableSourceMapCaching": {
              "type": "boolean",
              "description": "%node.enableSourceMapCaching.description%",
              "default": true
            }
          }
        }
      }
