    "tokenColors": [
      {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "foreground": "#969cbdB0",
          "fontStyle": ""
        }
      },
      {
        "scope": "meta.parameters comment.block",
        "settings": {
          "foreground": "#969cbd",
          "fontStyle": ""
        }
      },
      {
        "scope": ["variable.other.constant.object", "variable.other.readwrite.alias", "meta.import variable.other.readwrite"],
        "settings": {
          "foreground": "#0EBFFF"
        }
      },
      {
        "scope": ["variable.other.property", "variable.other", "support.type.object"],
        "settings": {
          "foreground": "#3b3e48"
        }
      },
      {
        "scope": ["meta.object.member"],
        "settings": {
          "foreground": "#0EBFFF"
        }
      },
      {
        "scope": ["variable - meta.import", "constant.other.placeholder", "variable.other.object.property", "meta.object-literal.key-meta.object.member"],
        "settings": {
          "foreground": "#3b3e48"
        }
      },
      {
        "scope": ["keyword.control.flow"],
        "settings": {
          "foreground": "#01DAB2"
        }
      },
      {
        "scope": ["keyword.operator.new"],
        "settings": {
          "foreground": "#01DAB2"
        }
      },
      {
        "scope": ["variable.language.super", "storage.modifier.async", "support.class.error", "keyword.control.trycatch", "keyword.operator.expression.delete", "keyword.operator.expression.void", "keyword.operator.void", "keyword.operator.delete"],
        "settings": {
          "foreground": "#ff2090"
        }
      },
      {
        "scope": ["variable.parameter", "variable.other.readwrite.js", "meta.definition.variable variable.other.constant", "meta.definition.variable variable.other.readwrite"],
        "settings": {
          "foreground": "#3b3e48"
        }
      },
      {
        "scope": ["constant.other.color"],
        "settings": {
          "foreground": "#000000"
        }
      },
      {
        "scope": ["invalid", "invalid.illegal"],
        "settings": {
          "foreground": "#ff2090"
        }
      },
      {
        "scope": ["invalid.deprecated"],
        "settings": {
          "foreground": "#ff2090"
        }
      },
      {
        "scope": ["keyword.control", "keyword"],
        "settings": {
          "foreground": "#969cbd"
        }
      },
      {
        "scope": ["keyword.operator", "storage.type", "storage.modifier", "keyword.control.conditional", "keyword.control.loop"],
        "settings": {
          "foreground": "#0EBFFF"
        }
      },
      {
        "scope": ["keyword.control.module", "keyword.control.import", "keyword.control.export", "keyword.control.default", "meta.import", "meta.export"],
        "settings": {
          "foreground": "#01DAB2"
        }
      },
