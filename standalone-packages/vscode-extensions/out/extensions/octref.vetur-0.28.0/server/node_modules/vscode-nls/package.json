	"devDependencies": {
		"@types/node": "^12.12.29",
		"@types/mocha": "^7.0.2",
		"mocha": "^7.1.0",
		"typescript": "^3.8.3",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.24.0",
		"rimraf": "^3.0.2"
